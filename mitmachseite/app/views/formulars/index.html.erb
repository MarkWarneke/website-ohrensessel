<p id="notice"><%= notice %></p>

<h1>Listing Formulars</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Street</th>
      <th>Zip</th>
      <th>Site</th>
      <th>Information</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @formulars.each do |formular| %>
      <tr>
        <td><%= formular.first_name %></td>
        <td><%= formular.last_name %></td>
        <td><%= formular.street %></td>
        <td><%= formular.zip %></td>
        <td><%= formular.site %></td>
        <td><%= formular.information %></td>
        <td><%= link_to 'Show', formular %></td>
        <td><%= link_to 'Edit', edit_formular_path(formular) %></td>
        <td><%= link_to 'Destroy', formular, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Formular', new_formular_path %>
