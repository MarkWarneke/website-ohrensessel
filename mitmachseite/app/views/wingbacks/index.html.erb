<p id="notice"><%= notice %></p>

<h1>Listing Wingbacks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wingbacks.each do |wingback| %>
      <tr>
        <td><%= link_to 'Show', wingback %></td>
        <td><%= link_to 'Edit', edit_wingback_path(wingback) %></td>
        <td><%= link_to 'Destroy', wingback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wingback', new_wingback_path %>
